    usuarios.map(({ id, username, nombre, apellidos, email, contrasena }) => (
    <article>
        <p>{id}</p>
        <p>{username}</p>
        <p>{nombre}</p>
        <p>{apellidos}</p>
        <p>{email}</p>
        <p>{contrasena}</p>
    </article>
    ))
}



